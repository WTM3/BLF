    "configuration": {
      "title": "BLF NJSON Engine",
      "properties": {
        "blf.heatShieldEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable heat shield protection against invalid inputs"
        },
        "blf.maxInputSize": {
          "type": "number",
          "default": 15000,
          "description": "Maximum input size before heat shield activation (bytes)"
        },
        "blf.bufferValue": {
          "type": "number",
          "default": 0.1,
          "description": "The narrow bridge constant (AIc + buffer = BMqs)"
        },
        "blf.maxFileSize": {
          "type": "number",
          "default": 1048576,
          "description": "Maximum file size for automatic processing (bytes)"
        },
        "blf.showComplexityWarnings": {
          "type": "boolean",
          "default": true,
          "description": "Show warnings when high complexity is detected"
        },
        "blf.showEngineStatus": {
          "type": "boolean",
          "default": true,
          "description": "Show engine status notifications"
        },
        "blf.autoStartFileWatcher": {
          "type": "boolean",
          "default": true,
          "description": "Automatically start file watcher on activation"
        }
      }
